//Write a function that takes the input array of strings and returns the longest common prefix as a string.
//Example:
//Input: ["flower", "flow", "flight"]
//Output: "fl"


public class commonSubString {

	public static void main(String[] args) {
		
		String[] str={"mnno", "nano", "rano"};
		String e1="";
		String e2="";
		String e3="";
		
		
			for(int j=1;j<4;j++)
			{
				e1= str[0].substring(0, j);
				e2= str[1].substring(0, j);
				e3= str[2].substring(0, j);
				
				if(e1.equals(e2) && e2.equals(e3))
				{
					
					System.out.println(e1+" is the substring");
				}
				continue;
				
				
				
			}
			
			
		
	
	}

}
