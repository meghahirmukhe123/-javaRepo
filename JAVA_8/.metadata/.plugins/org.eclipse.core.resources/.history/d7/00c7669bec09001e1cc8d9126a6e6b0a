package dateAndTimeAPI;

import java.time.LocalDateTime;

//if we want to get date and time before after particular month,day,year

public class findBeforeAfterdayTime {
	public static void main(String[] args)
	
	{
		LocalDateTime lt= LocalDateTime.of(1989,12,11,04,13,34,20);
		
		System.out.println(lt.plusMonths(6));  //it will give date time after 6 months of given month
		
		System.out.println()lt.minusMonths(6));  //it will give date time before 6 months of given month
		
		
	}

}
